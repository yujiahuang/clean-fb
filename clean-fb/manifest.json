{
  "manifest_version": 2,
  "name": "Clean Facebook",
  "short_name": "Clean FB",
  "description": "讓臉書版面他媽的別那麼擠，然後段落間加上他媽的空格。",
  "version": "1.6",
  "content_scripts": [
    {
      "matches": [
        "https://facebook.com/*", "https://www.facebook.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
