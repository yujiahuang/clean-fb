{
  "manifest_version": 2,
  "name": "Facebook Small Font",
  "short_name": "FB Small",
  "description": "Make the font of status on facebook small. Just like the good old days.",
  "version": "1.3",
  "content_scripts": [
    {
      "matches": [
        "https://facebook.com/*", "https://www.facebook.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
